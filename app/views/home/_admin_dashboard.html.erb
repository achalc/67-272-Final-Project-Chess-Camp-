<div class="row">
    <div class="large-6 columns">
        <center><h4> Camps per Instructor  </h4></center>
      <%=  bar_chart @camps_to_instructor, library: {backgroundColor: "#E6E6E6", colors: ['ffd519']} %>
    </div>
    
    <div class="large-6 columns">
      <center><h4> Camps per Curriculum</h4></center>
      <%=  pie_chart @camps.group(:name).count, library: {backgroundColor: "#E6E6E6" } %>
    </div>  
</div>

<div class="row">
  <div class="large-12 columns">
    <center><h4> Registrations by Camp & Date </h4></center>
    <%=  column_chart @camp_to_registration_admin, library: {backgroundColor: "#E6E6E6", colors: ['42ffe3']} %>
  </div>
</div>

